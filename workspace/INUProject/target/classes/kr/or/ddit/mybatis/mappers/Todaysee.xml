	<!-- 오늘 본 인재 로그 저장 및 24시간 내 확인
	TodaySeeVO(todayNo=0, prNo=32, memId=abcabc001, TODAY_DATE=null, TODAY_EDATE=null)
	 -->
	<insert id="insertTodaySee" parameterType="kr.or.ddit.selfpr.vo.TodaySeeVO">
		INSERT INTO TODAYSEE(TODAY_NO, PR_NO, MEM_ID, TODAY_DATE, TODAY_EDATE)
		VALUES(TODAYSEE_SEQ.NEXTVAL, #{prNo}, #{memId},SYSDATE, SYSDATE+1)
	</insert>


