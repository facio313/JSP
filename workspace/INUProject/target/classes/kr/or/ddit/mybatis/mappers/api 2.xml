<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.or.ddit.api.dao.ApiDAO">
	<insert id="insertCommonCode" parameterType="CommonCodeVO">
		INSERT INTO COMMON_CODE
		VALUES(
			#{code},#{codeName}
		<choose>
   			<when test="refCode neq null and @org.apache.commons.lang3.StringUtils@isNotBlank(refCode)">
   				, #{refCode}
   			</when>
   			<otherwise>
   				, null
   			</otherwise>
   		</choose>
		<choose>
   			<when test="codeDesc neq null and @org.apache.commons.lang3.StringUtils@isNotBlank(codeDesc)">
   				, #{codeDesc}
   			</when>
   			<otherwise>
   				, null
   			</otherwise>
   		</choose>
   		)
	</insert>
	<insert id="insertRegionCode">
		INSERT INTO REGION
		VALUES(
			#{regionCode},#{regionName}
		<choose>
   			<when test="regionRef neq null and @org.apache.commons.lang3.StringUtils@isNotBlank(regionRef)">
   				, #{regionRef}
   			</when>
   			<otherwise>
   				, null
   			</otherwise>
   		</choose>
   		)
	</insert>
	<insert id="insertJobCode">
		INSERT INTO ANNO_JOB
		VALUES(
			#{jobCode},#{jobName}
		<choose>
   			<when test="jobRef neq null and @org.apache.commons.lang3.StringUtils@isNotBlank(jobRef)">
   				, #{jobRef}
   			</when>
   			<otherwise>
   				, null
   			</otherwise>
   		</choose>
   		)
	</insert>
	
</mapper>