<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN" "http://tiles.apache.org/dtds/tiles-config_3_0.dtd" >
<tiles-definitions>
  <definition name="parent" template="/WEB-INF/templates/template.jsp" preparer="commonViewPreparer">
  	<put-attribute name="preScript" value="/includee/preScript.jsp" />
  	<put-attribute name="headerMenu" value="/includee/headerMenu.jsp" />
  	<put-attribute name="footer" value="/includee/footer.jsp" />
  	<put-attribute name="postScript" value="/includee/postScript.jsp" />
  	<put-attribute name="cssSource" value="/includee/cssScriptSources/css.jsp" />
  	<put-attribute name="scriptSource" value="/includee/cssScriptSources/script.jsp" />  	
  </definition>
  
  <!-- 사이드바 없는 템플릿 필요해서 만들었습니다. 근주 -->
  <definition name="expert" template="/WEB-INF/templates/expertTemplate.jsp" preparer="commonViewPreparer">
  	<put-attribute name="preScript" value="/includee/preScript.jsp" />
  	<put-attribute name="cssSource" value="/includee/cssScriptSources/css.jsp" />
  	<put-attribute name="postScript" value="/includee/postScript.jsp" />
  	<put-attribute name="scriptSource" value="/includee/cssScriptSources/script.jsp" />
  </definition>
  
  
  <definition name="expert/*" extends="expert">
  	<put-attribute name="content" value="/WEB-INF/views/expert/expertPay.jsp"/>
  </definition>
  <definition name="expert/*" extends="parent">
  	<put-attribute name="content" value="/WEB-INF/views/expert/{1}.jsp"/>
  </definition>
</tiles-definitions>
